{
  "name": "cocobakes",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:analyze": "astro build && npx source-map-explorer dist/**/*.js --html build-analysis.html",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint src --ext .ts,.tsx,.astro",
    "lint:fix": "eslint src --ext .ts,.tsx,.astro --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,astro,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,astro,css,md}\"",
    "typecheck": "astro check",
    "clean": "rm -rf dist node_modules/.cache",
    "check:all": "pnpm lint && pnpm typecheck && pnpm build"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/react": "^3.0.7",
    "@astrojs/tailwind": "^5.0.2",
    "@fontsource/dancing-script": "^5.0.8",
    "@fontsource/fredoka": "^5.0.8",
    "@iconify-json/fe": "^1.1.7",
    "@iconify-json/heroicons": "^1.1.13",
    "@iconify/react": "^4.1.1",
    "astro": "^3.5.4",
    "astro-icon": "1.0.0-next.2",
    "class-variance-authority": "^0.7.0",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  }
}
